{
    "compilerOptions": {
      "jsx": "react",
      "lib": ["es2015", "es2017", "dom"],
      "module": "es6",
      "moduleResolution": "node",
      "noImplicitAny": false,
      "outDir": "./wwwroot/dist/",
      "sourceMap": true,
      "target": "es5",
      "allowSyntheticDefaultImports": true
